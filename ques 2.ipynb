   "source": [
    "#Write a Python Program to print full pyramid using '*' symbol\n",
    "rows = 9\n",
    "i = 1\n",
    "while i<=rows:\n",
    "    j = 1\n",
    "    while j<=i:\n",
    "        print((\"*\"),end=\"\")\n",
    "        j = j+1\n",
    "    i = i+2\n",
    "    print('')"
   ]
